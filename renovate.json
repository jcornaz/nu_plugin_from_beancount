{
  "extends": [
    "config:base",
    ":automergePatch",
    "schedule:weekends",
    ":semanticCommitTypeAll(chore)"
  ],
  "enabled": false,
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchDepTypes": [
        "dev-dependencies"
      ],
      "automerge": true
    },
    {
      "matchPackagePatterns": ["^nu-"],
      "groupName": "nushell"
    }
  ]
}
